{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.akerBP.enterprise/authorized-mcp-servers",
  "description": "AkerBP Approved MCP Servers Whitelist - Only these servers are authorized for use",
  "title": "AkerBP MCP Server Whitelist",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/AkerBP/mcp-registry",
    "source": "github",
    "id": "AkerBP/mcp-registry"
  },
  "packages": [
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "@modelcontextprotocol/server-github",
      "version": "0.6.0",
      "transport": {
        "type": "stdio"
      },
      "_meta": {
        "approvedBy": "IT-Security",
        "approvalDate": "2025-10-31",
        "purpose": "GitHub repository management and issue tracking",
        "accessLevel": "authorized"
      }
    },
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "@modelcontextprotocol/server-filesystem",
      "version": "0.6.0",
      "transport": {
        "type": "stdio"
      },
      "_meta": {
        "approvedBy": "IT-Security",
        "approvalDate": "2025-10-31",
        "purpose": "File system access to /akerbp/shared directories",
        "accessLevel": "authorized"
      }
    },
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "markitdown-mcp-npx",
      "version": "0.0.1",
      "transport": {
        "type": "stdio"
      },
      "_meta": {
        "approvedBy": "IT-Security",
        "approvalDate": "2025-10-31",
        "purpose": "Document conversion to Markdown",
        "accessLevel": "authorized"
      }
    }
  ],
  "_meta": {
    "io.modelcontextprotocol.registry/publisher-provided": {
      "organization": "AkerBP",
      "registryType": "whitelist",
      "description": "Curated list of approved MCP servers for AkerBP organization",
      "securityLevel": "enterprise",
      "maintenanceOwner": "IT-Security-Team",
      "lastUpdated": "2025-10-31T00:00:00Z"
    }
  }
}
