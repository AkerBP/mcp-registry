{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.akerBP.enterprise/registry-config",
  "description": "AkerBP Enterprise MCP Server Registry Configuration",
  "title": "AkerBP MCP Registry",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/AkerBP/mcp-registry",
    "source": "github",
    "id": "AkerBP/mcp-registry"
  },
  "packages": [
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "@modelcontextprotocol/server-github",
      "version": "0.6.0",
      "transport": {
        "type": "stdio"
      }
    },
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "@modelcontextprotocol/server-filesystem",
      "version": "0.6.0",
      "transport": {
        "type": "stdio"
      }
    },
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "markitdown-mcp-npx",
      "version": "0.0.1",
      "transport": {
        "type": "stdio"
      }
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://raw.githubusercontent.com/modelcontextprotocol/servers/main/src/github/index.js",
      "headers": [
        {
          "name": "Authorization",
          "description": "GitHub Personal Access Token",
          "isRequired": true,
          "isSecret": true
        }
      ]
    },
    {
      "type": "streamable-http",
      "url": "https://raw.githubusercontent.com/modelcontextprotocol/servers/main/src/filesystem/index.js",
      "headers": []
    }
  ],
  "_meta": {
    "io.modelcontextprotocol.registry/publisher-provided": {
      "tool": "akerbp-enterprise-registry",
      "version": "1.0.0",
      "build_info": {
        "maintainer": "AkerBP IT Department",
        "contact": "it-support@akerbp.com", 
        "mcp_gateway": "mcp.akerbp.com",
        "sso_integration": "AkerBP Azure AD",
        "timestamp": "2025-10-31T00:00:00Z",
        "environment": "akerbp-production",
        "compliance": "ISO-27001, SOC-2",
        "data_residency": "Norway/EU"
      }
    }
  }
}
